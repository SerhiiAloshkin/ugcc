<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0//EN"
        "http://google-web-toolkit.googlecode.com/svn/releases/2.0/distro-source/core/src/gwt-module.dtd">
<module rename-to="ugcc">

    <!-- Inherit the core Web Toolkit stuff.                  -->
    <inherits name='com.google.gwt.user.User'/>
    <inherits name="com.google.gwt.place.Place"/>
    <inherits name="com.google.gwt.activity.Activity"/>
    <inherits name="ua.coral.ugcc.common.Common"/>

    <!-- Specify the app entry point class.                   -->
    <entry-point class='ua.coral.ugcc.user.client.Ugcc'/>

<!-- Specify the app servlets.                   -->
    <servlet path='/ugccService' class='ua.coral.ugcc.user.server.UgccServiceImpl'/>

    <replace-with class="ua.coral.ugcc.common.view.impl.ClientFactoryImpl">
        <when-type-is class="ua.coral.ugcc.common.view.ClientFactory"/>
    </replace-with>

    <replace-with class="ua.coral.ugcc.user.client.view.impl.MainViewImpl">
        <when-type-is class="ua.coral.ugcc.common.view.MainView"/>
    </replace-with>

    <replace-with class="ua.coral.ugcc.user.client.view.impl.NewsViewImpl">
        <when-type-is class="ua.coral.ugcc.common.view.NewsView"/>
    </replace-with>
    
</module>